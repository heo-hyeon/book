<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="nov.infra.modules.list.ListMapper">

	<resultMap id="resultMapObj" type="nov.infra.modules.list.List"></resultMap>
	
	<select id="selectList" resultMap="resultMapObj">
	
		SELECT 
			a.* 
		FROM member a
		WHERE 1=1
			A
		
	
	</select>

</mapper>
    